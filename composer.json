{
	"name"       : "renatonascalves/wp-mailtrap",
	"description": "WP Mailtrap is a simple plugin to test emails in WordPress with the Mailtrap API..",
	"homepage"   : "https://github.com/renatonascalves/wp-mailtrap",
	"type"       : "wordpress-plugin",
	"license"    : "GPL-2.0+",
	"keywords"   : [ "mail", "mailtrap", "testing" ],
	"support"    : {
		"issues": "https://github.com/renatonascalves/wp-mailtrap/issues",
		"source": "https://github.com/renatonascalves/wp-mailtrap"
	},
	"authors" : [
		{
			"name"    : "Renato Alves",
			"homepage": "http://ralv.es/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
	    "files": [ "wp-mailtrap.php" ]
	},
	"require": {
		"php": ">=5.2"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "*",
		"wimg/php-compatibility": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
	}
}
